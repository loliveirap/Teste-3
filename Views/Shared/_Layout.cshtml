<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>OBJETIVO Conveniadas</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/templateMaster.css" rel="stylesheet" />
</head>
@*#FFCC00 #fff200
    #004a8f*@
<body>
    <div class="navbar navbar-inverse navbar-fixed-top container body-content templatemenu" style="background-color:#004a8f">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a href='@Url.Action("Index", "Home")' class="navbar-brand"><img src="~/Content/img/obj.png" width="140" /></a>
                @*@Html.ActionLink("OBJETIVO", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })*@
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    @*<li>@Html.ActionLink("CONVENIADAS", "Cadastrar", "Conveniada")</li>
                        <li>@Html.ActionLink("PREFEITURAS", "Cadastrar", "Prefeitura")</li>*@
                </ul>
                <ul class="nav navbar-nav navbar-right">
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false" title="Minha Conta"><span class="glyphicon glyphicon-user" aria-hidden="true" style="font-size: 20px;"></span> <span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li><a><b>Nome:</b> Departamento Convênios</a></li>
                            <li><a><b>Usuário:</b> CONVENIOS</a></li>
                            <li><a><b>Setor:</b> Departamento Convênios</a></li>
                        </ul>
                    </li>
                    <li>
                        @*@Html.ActionLink("SAIR", "LogOn", "Autenticacao")*@
                    <a class="active" title="Sair"
                       href="@Url.Action("LogOff", new { controller = "Autenticacao"})">
                        <span class="glyphicon glyphicon-log-out" style="font-size: 20px;"></span>
                    </a>
                </li>
            </ul>
        </div>
    </div>
</div>

<div class="container body-content">
    @RenderBody()
    <hr />
    <footer>
        <p>&copy; @DateTime.Now.Year - OBJETIVO</p>
    </footer>
</div>

@*@Scripts.Render("~/bundles/jquery")*@
<script src="~/Scripts/jquery-1.10.2.min.js"></script>
@Scripts.Render("~/bundles/bootstrap")
@RenderSection("scripts", required: false)
</body>
</html>
